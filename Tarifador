                            sscanf(cad,"%d/%d/%d %d:%d", &dia, &mes, &anio, &hora, &min);

                             if((dia == 16 && mes == 2)||
                               (dia == 17 && mes == 2)||
                               (dia == 23 && mes == 3)||
                               (dia == 24 && mes == 3)||
                               (dia == 2  && mes == 4)||
                               (dia == 3  && mes == 4)||
                               (dia == 1  && mes == 5)||
                               (dia == 25 && mes == 5)||
                               (dia == 20 && mes == 6)||
                               (dia == 9  && mes == 7)||
                               (dia == 7  && mes ==12)||
                               (dia == 25 && mes ==12))
                            codigo_dia = 100;
                        else
                            codigo_dia = calcular_dia(dia,mes,anio);
                            switch(codigo_dia)
                            {
                                case 0://DOMINGO
                                    {
                                        if(clave == 1)
                                        {
                                            unidades = (dur/240) + 1;
            //                                printf("\n Unidades: %d", unidades);
                                            costo_clave1 = unidades * costo_min;
